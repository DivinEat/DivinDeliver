{% extends 'base_back.html.twig' %}

{% block title %}User {{ user.id }}{% endblock %}

{% block body %}
    Id : {{ user.id }} <br>
    Email : {{ user.email }} <br>
    Roles : {{ user.roles ? user.roles|json_encode : '' }} <br>
    Password : {{ user.password }} <br>
    <a href="{{ path('admin_user_edit', {id: user.id}) }}">Editer</a> <br>
    <br>
    <a href="{{ path('admin_user_index') }}">Retour aux users</a>
{% endblock %}
